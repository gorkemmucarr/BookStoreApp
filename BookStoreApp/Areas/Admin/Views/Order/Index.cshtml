@model IEnumerable<Order>

@if (Model.Count() > 0)
{
    @foreach (var orders in Model)
    {
        <div class="my-5">
            <partial name="_OrderDetails" model="@orders" />
        </div>
    }
}
else
{
    <div class="alert alert-info">
        <p class="lead">There is no order</p>
    </div>
}